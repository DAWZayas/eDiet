   {
   "parser": "babel-eslint",
   "extends": "airbnb",
   "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "destructuring": true,
            "jsx": true
        }
   },
   "rules": {
     "object-curly-spacing": ["warn", "never"],
     "func-names": "off",
     "space-before-function-paren": ["error", "never"],
     "max-len": ["error", 120, 4]
   }

 }
